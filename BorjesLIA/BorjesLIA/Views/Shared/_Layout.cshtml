<!DOCTYPE html>
<html>
<head>
    <link href='https://fonts.googleapis.com/css?family=Raleway' rel='stylesheet' type='text/css'>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Börjes Transport</title>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.2/jquery.min.js"></script>
    @*<script src="~/Scripts/daJavaScript.js"></script>*@
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/production/css/custom.css" rel="stylesheet" />
   

    
    <link href="~/Content/font-awesome.css" rel="stylesheet">
    @*<link href="~/Content/animate.min.css" rel="stylesheet">*@
    <link href="~/Content/main.css" rel="stylesheet">

    <!--bxSlider-->
    <link href="~/Content/jquery.bxslider.css" rel="stylesheet" />
    <!--bxSlider End-->

</head>
<body>
    <div id="#navbar-start" class="navbar navbar-default navbar-fixed-top">
        <div class="container-dashboard">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <div>
                    <a href="/"><img src="~/Content/images/borjes-logga-jpg.jpg" /></a>
                </div>
            </div>
            <div class="navbar-collapse collapse">
                @Html.Partial("_LoginPartial")
                <span class="navbar-right navbar-form btn fa fa-tv fa-2x" onclick="toggleFullScreen(elem)" data-toggle="tooltip" data-placement="left" title="Helskärm"></span>
                @*<span class="navbar-right navbar-form btn fa fa-tv fa-2x" data-toggle="tooltip" data-placement="left" title="Helskärm"></span>*@

            </div>
        </div>
    </div>
    <div class="container-max">
        @RenderBody()
    </div>


    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Content/production/js/custom.js"></script>
    <script src="~/Scripts/main.js"></script>

    @RenderSection("scripts", required: false)
</body>
</html>
