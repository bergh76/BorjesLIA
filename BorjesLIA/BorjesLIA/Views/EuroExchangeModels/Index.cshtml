                pointHighlightStroke: "rgba(151,187,205,1)",
                data: [28, 48, 40, 19, 96, 27, 100]
            }]
        };

        var canvasRadar = new Chart(ctx, {
            type: 'radar',
            data: data,
        });

        // Pie chart
        var ctx = document.getElementById("pieChart");
        var data = {
            datasets: [{
                data: [120, 50, 140, 180, 100],
                backgroundColor: [
                  "#455C73",
                  "#9B59B6",
                  "#BDC3C7",
                  "#26B99A",
                  "#3498DB"
                ],
                label: 'My dataset' // for legend
            }],
            labels: [
              "Dark Gray",
