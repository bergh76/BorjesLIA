@model BorjesLIA.ViewModel.StartModelViewModel
@{
    string stringPartOfIdIndex = "carouselElem";
    int IdIndex = 0;
    string OwlItemIndexID = String.Format("{0},{1}", stringPartOfIdIndex, IdIndex);
}

<nav class="navbar navbar-default navbar-collapse">
    <!-- testade att byta till div ist, för att ändra placeringen och få med naven/ -->

    <div class="container-dashboard">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <div>
                <a href="/">
                    <img src="~/Content/images/logo_blu.png" />
                    @*<img src="~/Content/images/borjes-logga-jpg.jpg" />*@
                </a>
            </div>
        </div>
        <div class="navbar-collapse collapse">
            @Html.Partial("_LoginPartial")
            <span  onmouseover="openNav()"><i class="glyphicon glyphicon-list fa-2x pull-right" style="padding-top:10px; cursor: pointer"></i></span>
            @*<span class="navbar-right navbar-form btn fa fa-television fa-2x" onclick="toggleFullScreen()" data-toggle="tooltip" data-placement="left" title="Helskärm"></span>*@
        </div>
    </div>
</nav>
<section id="main-slider">
        <div class="owl-carousel">
            @foreach (var element in Model.listVM.OrderBy(x => x.PlacingOrder))
            {
                //för att lokal video inte går att styra från iframe.
                if (element.Type == 7) //7 typ för mp4
                {
                    <div class="owl-item">
                        <video id="@element.Url" class="center">
                            <source src="@element.Url" type="video/mp4" class="VideoClassTag" name="@element.Duration" />
                        </video>
                    </div>
                }
                else
                {
                    <div class="owl-item">
                        <iframe id="@element.Url" class="VideoClassTag" src="@element.Url" name="@element.Duration" webkitAllowFullScreen mozallowfullscreen allowFullScreen allowfullscreen frameborder="0"></iframe>
                    </div>
                }
            }
        </div>
    </section>

