@model BorjesLIA.ViewModel.StartModelViewModel

<!--bxSlider -->
@*<script src="~/Scripts/jquery.bxslider.min.js"></script>*@
<!--testat att flytta dajavascript kod till mainjs-->
<script src="~/Scripts/daJavaScript.js"></script> 

<div class="main_container">
    <div id="bx-wrapper">
        <!-- using the elements in the models list to represent each slide in slider. -->
        <ul class="bxslider">
            @foreach (var element in Model.listVM.OrderBy(x => x.orderby))
            {
                <li><iframe src="@element.url" webkitAllowFullScreen mozallowfullscreen allowFullScreen frameborder="0"></iframe></li>
            }
        </ul>
    </div>
</div>

